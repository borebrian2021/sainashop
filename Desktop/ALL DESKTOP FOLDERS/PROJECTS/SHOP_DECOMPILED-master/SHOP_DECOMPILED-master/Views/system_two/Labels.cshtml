                <h3 class="text">Labels generator</h3>
                <button onclick="print_sample()">Print</button>
                <hr />
                <img src="data:image/png;base64," style="height:100px;" />
                <button data-toggle="modal" data-target="#add_items" type="submit" class="btn btn-default button-general m-2 w-100 text-centre"> <i class="fa fa-plus"></i> &nbsp; Add item</button>

            </div>
        </div>
    </div>
    <div class="col-lg-8">
        <div class="card_log_in text-center">
            <div class="container-fluid text-center">
                <h2 class="text">Availlable Items</h2>
                <hr />
                <img src="https://www.seekpng.com/png/full/957-9573156_general-store-items-png.png" style="height:100px;" />
                <br />
                <br />

                <div id="del_1" class="w3-container city">

                    <div class="card_tabs text-center p-2">
                        <div class=" text-center p-2">
                            <table id="deliveries" class="table table-striped table-bordered table-sm " cellspacing="0" width="100%">
                                <thead>
                                    <tr>
                                        <th class="th-sm">
                                            Item No.
                                        </th>

                                        <th class="th-sm">
                                            Date
                                        </th>
                                        <th class="th-sm">
                                            Item Name
                                        </th>
                                        <th class="th-sm">
                                            Quantity
                                        </th>
                                        <th class="th-sm">
                                            Price
                                        </th>

                                        <th class="th-sm">
                                            Actions
                                        </th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <tr>
                                        <td><img src="https://freepngimg.com/thumb/shopping/13-2-shopping-free-png-image-thumb.png" class="round" /></td>
                                        <td>5465456</td>
                                        <td>05/10/2022</td>
                                        <td>Kiwi Small</td>
                                        <td>20</td>
                                        <td>45/=</td>
                                        <td>


                                            <button type="button" class="btn btn-light dropdown-toggle btn-sm custome_bt" data-toggle="dropdown">
                                                More  <span class="caret"></span>
                                            </button>
                                            <ul class="dropdown-menu container-fluid" role="menu" style="border-radius:20px;box-shadow: rgba(0, 0, 0, 0.24) 0px 3px 8px;border:none;">
                                                <li style="font-size:20px;width:100%"><a data-target="#printL" data-toggle="modal" class="link_1" onclick="generateBarcode_('4567655','KIWI 25g','Ksh. 45');"><i class="fa fa-print"></i>&nbsp;Print lebel(s)</a></li>
                                                <li style="font-size:20px;width:100%"><a class="link_2" onclick="modify();"> <i class="fa fa-plus"></i>&nbsp;Add items</a></li>
                                                <li style="font-size:20px;width:100%"><a asp-action="delete_client" asp-controller="home" class="link_3"><i class="fa fa-database"></i>&nbsp;Delete</a></li>


                                            </ul>
                                        </td>
                                    </tr>
                                </tbody>

                            </table>


                        </div>
                    </div>
                </div>


            </div>
        </div>
    </div>

</div>

@*<button onclick="googleSignin()">Google Signin</button>
    <button onclick="googleSignout()">Google Signout</button>*@

<div class="modal fade" id="add_items" tabindex="-1" role="dialog" aria-labelledby="exampleModalCenterTitle" aria-hidden="true">
    <div class="modal-dialog " role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="exampleModalLongTitle">
                    @*Add items to system*@
                    <button  class="btn btn-danger" onclick="print_sample();">Print</button>

                </h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <form asp-action="receipt">

                    <label>Enter Item name</label>
                    <input class="form-control" name="item_name" required placeholder="Enter item name" type="text" />

                    @{
                        Random rnd = new Random();
                        int num = rnd.Next(000000, 999999);


                    }
                    <input class="form-control" name="no" value="@num" hidden type="text" />
                    <label>Enter Date:</label>
                    <input class="form-control" name="date" required placeholder="Enter date here" type="date" />

                    <label>Enter Quantity:</label><br />
                    <input class="form-control" name="date" required placeholder="Enter quantity" vlaue="0" name="cash" type="number" />
                    <label>Enter price:</label>
                    <input class="form-control" name="date" required placeholder="Enter price" name="price" type="number" />




                    <button type="submit" class="btn btn-success m-3">Submit</button>



                </form>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>

            </div>
        </div>
    </div>
</div>
<dilv class="modal fade" id="printL" tabindex="-1" role="dialog" aria-labelledby="exampleModalCenterTitle" aria-hidden="true">
    <div class="modal-dialog " role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="exampleModalLongTitle">Labels Printing </h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <div id="generator">
                    <form id="form">
                        <input type="text" id="barcodeValue" hidden>
                        <div id="config" style="display:none">
                            <div class="config">
                                <div class="title">Type</div>
                                <input type="radio" name="btype" id="ean8" value="ean8" checked="checked"><label for="ean8">EAN 8</label><br />
                                <input type="radio" name="btype" id="ean13" value="ean13"><label for="ean13">EAN 13</label><br />
                                <input type="radio" name="btype" id="upc" value="upc"><label for="upc">UPC</label><br />
                                <input type="radio" name="btype" id="std25" value="std25"><label for="std25">standard 2 of 5 (industrial)</label><br />
                                <input type="radio" name="btype" id="int25" value="int25"><label for="int25">interleaved 2 of 5</label><br />
                                <input type="radio" name="btype" id="code11" value="code11"><label for="code11">code 11</label><br />
                                <input type="radio" name="btype" id="code39" value="code39"><label for="code39">code 39</label><br />
                                <input type="radio" name="btype" id="code93" value="code93"><label for="code93">code 93</label><br />
                                <input type="radio" name="btype" id="code128" value="code128"><label for="code128">code 128</label><br />
                                <input type="radio" name="btype" id="codabar" value="codabar"><label for="codabar">codabar</label><br />
                                <input type="radio" name="btype" id="msi" value="msi"><label for="msi">MSI</label><br />
                                <input type="radio" name="btype" id="datamatrix" value="datamatrix"><label for="datamatrix">Data Matrix</label><br /><br />
                            </div>

                            <div class="config">
                                <div class="title">Misc</div>
                                Background : <input type="text" id="bgColor" value="#FFFFFF" size="7"><br />
                                "1" Bars : <input type="text" id="color" value="#000000" size="7"><br />
                                <div id="barcode1D">
                                    bar width: <input type="text" id="barWidth" value="1" size="3"><br />
                                    bar height: <input type="text" id="barHeight" value="30" size="3"><br />
                                </div>
                                <div id="barcode2D">
                                    Module Size: <input type="text" id="moduleSize" value="5" size="3"><br />
                                    Form: <input type="checkbox" name="rectangular" id="rectangular"><label for="rectangular">Rectangular</label><br />
                                </div>
                                <div id="miscCanvas">
                                    x : <input type="text" id="posX" value="10" size="3"><br />
                                    y : <input type="text" id="posY" value="20" size="3"><br />
                                </div>
                            </div>

                            <div class="config">
                                <div class="title">Format</div>
                                <input type="radio" id="css" name="renderer" value="css" checked="checked"><label for="css">CSS</label><br />
                                <input type="radio" id="bmp" name="renderer" value="bmp"><label for="bmp">BMP (not usable in IE)</label><br />
                                <input type="radio" id="svg" name="renderer" value="svg"><label for="svg">SVG (not usable in IE)</label><br />
                                <input type="radio" id="canvas" name="renderer" value="canvas"><label for="canvas">Canvas (not usable in IE)</label><br />
                            </div>
                        </div>


                    </form>
                </div>
                <div class="barcode_older print p-1 text-centre" style="color:black;font-weight:900">
                    <small id="i_name" class="m-0" style="font-size:10px;color:black">SUPREME SAI ENT.</small><br />
                    <small id="i_name" class="m-0" style="font-size:10px;color:black">Kiwi 25Kg</small> &nbsp;| &nbsp;
                    <small id="i_name" class="m-0">45/=</small>
                    <div id="barcodeTarget" class="barcodeTarget "></div>
                    <canvas id="canvasTarget" width="50" height="50"></canvas>
                </div>
                <button type="button" class="btn btn-secondary btn-sm" onclick="window.print();return false;">Print</button>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-secondary btn-sm" data-dismiss="modal">Close</button>


            </div>
        </div>
    </div>
</dilv>
